<div class="container">
  <div class="row">
    <h1 class="col-sm-9">Wiki Listing</h1>
    <%= link_to 'Start a new wiki', new_wiki_path, class: 'btn btn-success col-sm-3' %>
  </div>
  <% @wikis.each do |wiki| %>
    <div class="row">
      <p><%= link_to wiki.title, wiki_path(wiki) %></p>
    </div>
  <% end %>
</div>
